START
   TYPE BASETYPE TABLE W 4;
      BEGIN
      ITEM ITEM1 B 16 POS(0,0);
      ITEM ITEM2 B 1  POS(3,0);
      ITEM ITEM3 U 4  POS(12,0);
      ITEM ITEM4 B 16 POS(0,1);
      ITEM ITEM5 B 16 POS(0,2);
      ITEM ITEM6 B 16 POS(0,3);
      END
   TYPE TABLEOFBASE TABLE(1:6) BASETYPE;
   TYPE PTRTOTABLE P TABLEOFBASE;
   DEF TABLE TABLE1 TABLEOFBASE;
   DEF TABLE TABLE2 TABLEOFBASE;
   DEF TABLE TABLE3 TABLEOFBASE;
   DEF TABLE PTRTABLE(0:2) PTRTOTABLE = LOC(TABLE1),LOC(TABLE2),LOC(TABLE3);
DEF PROC main;
BEGIN
   ITEM6(1)@PTRTABLE(1)=1B'0';
END
TERM
